<%= form_for @game do |f| %>
  <%= f.label :sport %>
  <%= f.text_field :sport %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :start_time %>
  <%= f.datetime_field :start_time %>
  <%= f.label :end_time %>
  <%= f.datetime_field :end_time %>
  <%= f.submit "Update Game" %>
<% end %>


<%= form_for @game, method: :delete, :class => :destroy do |f| %>
  <%= f.submit "Delete Game" %>
<% end %>

